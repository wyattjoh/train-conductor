{
  "name": "@wyattjoh/train-conductor",
  "version": "0.1.0",
  "license": "MIT",
  "exports": "./src/main.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "deno.json", "src/**/*.ts"],
    "exclude": ["src/**/*_test.ts", "src/**/*.test.ts"]
  },
  "tasks": {
    "dev": "deno run --allow-read --allow-write --allow-run --allow-env src/main.ts",
    "compile": "deno compile --allow-read --allow-write --allow-run --allow-env -o train-conductor src/main.ts",
    "install": "deno install --global --force --allow-read --allow-write --allow-run --allow-env --config deno.json -n train-conductor src/main.ts",
    "check": "deno check src/main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "test": "deno test --allow-read --allow-write --allow-run --allow-env"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/toml": "jsr:@std/toml@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/fmt": "jsr:@std/fmt@^1.0.0",
    "@std/cli": "jsr:@std/cli@^1.0.0",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7"
  },
  "compilerOptions": {
    "strict": true
  }
}
